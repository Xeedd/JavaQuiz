
public class Answer {

	private String answerText;
	private boolean isCorrect;
	
	
	public Answer(String answerText, boolean isCorrect) {
		this.answerText = answerText;
		this.isCorrect = isCorrect;
	}

	
	@Override
	public String toString() {
		return  answerText;
	}



	
	public boolean isCorrect() {
		return isCorrect;
	}
	
	

}
